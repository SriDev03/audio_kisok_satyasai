{
  "name": "audio_kiosk",
  "version": "0.1.0",
  "description": "Interactive Application",
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@felipebutcher/node-os-info": "0.0.6",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "electron-download-manager": "^2.1.2",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.4.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.3.9",
    "getmac": "^5.20.0",
    "https": "^1.0.0",
    "network": "^0.6.0",
    "newdb": "^1.6.0",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-audio-player": "^0.17.0",
    "react-bootstrap": "^2.1.1",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "shelljs": "^0.8.4",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron-forge start",
    "package": "react-scripts build && electron-forge package",
    "make": "react-scripts build && electron-forge make",
    "sass:build": "node-sass app/scss -o app/css",
    "sass:watch": "node-sass -w app/render/scss -o app/render/css",
    "postinstall": "electron-builder install-app-deps",
    "pack": "react-scripts build && electron-builder -l --publish never",
    "publish": "react-scripts build && electron-builder -l --publish always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "Shubham Sharma",
  "license": "Copyright Devgap",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "concurrently": "^5.2.0",
    "electron": "^13.1.0",
    "electron-builder": "^22.13.1",
    "electron-devtools-installer": "^3.2.0",
    "wait-on": "^5.1.0"
  },
  "build": {
    "productName": "MMS-Node",
    "artifactName": "${productName}-App.${ext}",
    "asar": false,
    "publish": {
      "provider": "s3",
      "bucket": "electronserver",
      "region": "ap-south-1",
      "path": "/ff-1-1-1"
    },
    "appId": "com.github.devgap.test",
    "linux": {
      "category": "test.app.development.type",
      "target": "AppImage"
    }
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ff-1-1-1"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
